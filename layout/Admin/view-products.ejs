
<div class="container">

  <div class="ps-5">
    <a href="/Admin/add-product"><button>ADD products</button></a>
  </div>
    <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">catagory</th>
            <th scope="col">name</th>
            <th scope="col">image</th>
          </tr>
        </thead>
        <tbody>
          <% for(var product of products){ %>  
          <tr>
            <th scope="row">1</th>
            <td><%= product.catagoty %></td>
            <td><%= product.name %></td>
            <td><img style="width: 80px; height: 50px;" src="<%= product.image %>" alt=""></td>
          </tr>
          <% } %>
        </tbody>
      </table>
</div>